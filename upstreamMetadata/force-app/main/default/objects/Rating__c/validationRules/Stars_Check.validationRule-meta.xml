<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Stars_Check</fullName>
    <active>true</active>
    <errorConditionFormula>Stars__c &lt; 1 &amp;&amp; Stars__c &gt; 5</errorConditionFormula>
    <errorDisplayField>Stars__c</errorDisplayField>
    <errorMessage>Stars should be an integer between 1 and 5 inclusively</errorMessage>
</ValidationRule>
